#include<iostream>
#include<vector>>
using namespace std;


int main() {

	vector<int>v(5, 100);
	vector<int>v2(6, 200);
	v.swap(v2);
	cout << v2[0] << endl;
	v.push_back(5);
	cout << "size = " << v.size() << endl;
	cout << "capacity = " << v.capacity() << endl;
	cout << v.empty()<<endl;
	for (size_t i = 0; i < v.size(); i++)
	{
		cout << v[i] << " ";
	}
}


//add back - delete back : O(1)
//add any where - delete any where : O(n)
//access : O(1)
//search : O(log N)
